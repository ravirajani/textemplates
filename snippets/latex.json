{
    "maths": {
        "prefix": "tex-maths",
        "body": [
            "\\usepackage{amsmath,amsthm}",
            "\\renewcommand\\dotsc{\\mathinner{\\ldotp\\kern-0.12em\\ldotp\\kern-0.12em\\ldotp}} % Tighter ellipsis between commas",
            "\\DeclareMathOperator{\\vol}{vol}",
            "\\usepackage{newtxmath} % Times font",
            "\\usepackage{mleftright} % \\mleft \\mright has no extra space around it",
            "\\usepackage{bm}",
            "\\newcommand\\dotp{\\mathbin{\\bm{\\cdot}}}",
            "\\usepackage{nicematrix}",
            "\\NiceMatrixOptions{renew-dots,renew-matrix,nullify-dots}",
            "\\usepackage{xfrac} % small fraction \\sfrac defined here",
            "\\newlength{\\customheadsep}",
            "\\setlength{\\customheadsep}{1.5ex plus 1.5ex minus .3ex}"
        ],
        "description": "Maths"
    },
    "theorem": {
        "prefix": "tex-theorem",
        "body": [
            "\\usepackage{xpatch}",
            "\\makeatletter",
            "\\xpatchcmd{\\proof}{\\@addpunct{.}}{}{}{}",
            "\\makeatother",
            "\\xpatchcmd{\\proof}{\\trivlist}{\\setlength{\\labelsep}{\\customheadsep}\\trivlist}{}{}",
            "\\xpatchcmd{\\proof}{\\itshape}{\\itshape\\bfseries}{}{}",
            "\\renewcommand{\\qedsymbol}{$\\blacksquare$}",
            "",
            "\\newtheoremstyle{plainthm}% name",
            "{}%      Space above, empty = `usual value'",
            "{}%      Space below",
            "{\\itshape}% Body font",
            "{}%         Indent amount (empty = no indent, \\parindent = para indent)",
            "{\\bfseries}% Thm head font",
            "{}%        Punctuation after thm head",
            "{\\customheadsep}%     Space after thm head: \" \" = normal interword space;",
            "      %       \\newline = linebreak",
            "{\\thmname{#1}\\thmnumber{ #2}\\thmnote{ \\normalfont{#3}}}% Thm head spec",
            "",
            "\\theoremstyle{plainthm}",
            "\\newtheorem{theorem}{Theorem}[section]",
            "",
            "\\newtheoremstyle{boldthm}% name",
            "{}%      Space above, empty = `usual value'",
            "{}%      Space below",
            "{\\itshape}% Body font",
            "{}%         Indent amount (empty = no indent, \\parindent = para indent)",
            "{\\bfseries}% Thm head font",
            "{}%        Punctuation after thm head",
            "{\\customheadsep}%     Space after thm head: \" \" = normal interword space;",
            "      %       \\newline = linebreak",
            "{\\thmname{#1}\\thmnumber{ #2}\\thmnote{ (#3)}}% Thm head spec",
            "",
            "\\theoremstyle{boldthm}",
            "\\newtheorem{btheorem}[theorem]{Theorem}",
            "",
            "\\newtheoremstyle{customex}% name",
            "{}%      Space above, empty = `usual value'",
            "{}%      Space below",
            "{}% Body font",
            "{}%         Indent amount (empty = no indent, \\parindent = para indent)",
            "{\\bfseries}% Thm head font",
            "{}%        Punctuation after thm head",
            "{\\customheadsep}%     Space after thm head: \" \" = normal interword space;",
            "      %       \\newline = linebreak",
            "{\\thmname{#1}\\thmnumber{ #2}\\thmnote{ (#3)}}% Thm head spec",
            "",
            "\\theoremstyle{customex}",
            "\\newtheorem{examplex}[theorem]{Example}",
            "\\newenvironment{example}%",
            "    {\\pushQED{\\qed}\\renewcommand{\\qedsymbol}{$\\blacktriangle$}\\examplex}%",
            "    {\\popQED\\endexamplex}",
            "",
            "\\numberwithin{equation}{section}",
            "",
            "\\usepackage{enumitem}",
            "\\newlist{parts}{enumerate}{2}",
            "\\setlist[parts,1]{label=\\normalfont{(\\alph*)},labelsep=.5em}",
            "\\setlist*[parts,2]{label=\\normalfont{(\\roman*)},labelsep=.5em}"
        ],
        "description": "Theorems and Examples"
    },
    "figure": {
        "prefix": "tex-figure",
        "body": [
            "\\numberwithin{figure}{section}",
            "",
            "\\usepackage{caption,floatrow}",
            "\\DeclareCaptionLabelSeparator{capspace}{\\hspace{\\customheadsep}}",
            "\\DeclareFloatStyle{figure}{capposition=bottom}",
            "\\floatsetup[figure]{style=figure}",
            "\\captionsetup[figure]{font=small,labelfont=bf,labelsep=capspace}"
        ],
        "description": "Figures"
    },
    "titlesec": {
        "prefix": "tex-titlesec",
        "body": [
            "\\usepackage[small,center]{titlesec}",
            "\\titleformat*{\\subsection}{\\filcenter\\itshape\\bfseries}",
            "\\titleformat{\\subsubsection}[runin]",
            "{\\normalfont\\bfseries}",
            "{\\thesubsection}{.33em}{}[.]",
            "\\titlespacing{\\subsubsection}{0pt}{1.5ex plus .1ex minus .2ex}{.5em}"
        ],
        "description": "Custom titles"
    },
    "complexity": {
        "prefix": "tex-complexity",
        "body": [
            "\\usepackage[classfont=sanserif,",
            "            langfont=caps,",
            "            funcfont=roman]{complexity}",
            "\\newlang{\\SVP}{SVP}"
        ],
        "description": "Complexity classes"
    },
    "algorithm": {
        "prefix": "tex-algorithm",
        "body": [
            "\\usepackage{algorithmicx,varwidth}",
            "\\makeatletter",
            "\\algnewcommand{\\MultiState}[1]{%",
            "    \\State\\begin{varwidth}[t]{\\dimexpr\\linewidth-\\ALG@thistlm}#1\\end{varwidth}%",
            "}",
            "\\patchcmd{\\ALG@doentity}{\\item[]\\nointerlineskip}{}{}{} % patch spurious space",
            "\\makeatother",
            "\\algdef{SN}[WHILE]{While}{EndWhile}[1]{\\textbf{while}\\ #1:}%",
            "\\algdef{SN}[FOR]{For}{EndFor}[1]{\\textbf{for}\\ #1:}%",
            "\\algdef{SN}[IF]{If}{EndIf}[1]{\\textbf{if}\\ #1:}%",
            "\\algdef{C}[IF]{IF}{ElseIf}[1]{\\textbf{else if}\\ #1:}%",
            "\\algdef{CN}[ELSE]{IF}{Else}{EndIf}{\\textbf{else}:}%",
            "\\algdef{SE}[REPEAT]{Repeat}{Until}{\\textbf{repeat}:}[1]{\\textbf{until}\\ #1}%"
        ],
        "description": "Custom algorithm pseudocode"
    },
    "summary": {
        "prefix": "tex-summary",
        "body": [
            "\\newenvironment{summary}",
            "{\\leftskip\\parindent\\relax\\rightskip\\parindent\\relax\\noindent\\ignorespaces}",
            "{\\par}"
        ],
        "description": "Summary environment definition"
    },
    "framedbox": {
        "prefix": "tex-framedbox",
        "body": [
            "\\usepackage{tcolorbox}",
            "% Example:",
            "% \\begin{algorithm}{Title}{label}",
            "%     Write boxed text here. Refer to it in the text with \\ref{alg:label}",
            "%     \\tcblower",
            "%     This is the lower part of the box.",
            "% \\end{algorithm}",
            "\\tcbuselibrary{skins,breakable,theorems}",
            "\\tcbset{enhanced jigsaw, breakable, sharp corners=all, fonttitle=\\bfseries,",
            "before skip=15pt plus 5pt minus 5pt, after skip=15pt plus 5pt minus 5pt,",
            "top=5pt, toptitle=1mm, bottom=1mm, left=1mm, right=1mm, middle=1mm,",
            "coltitle=black, titlerule=0pt, colback=white, colbacktitle=white}",
            "\\newtcbtheorem[number within=section]{algorithm}{Algorithm}%",
            "{boxrule=0mm, toprule=1pt, bottomrule=1pt,",
            "theorem hanging indent=0pt,",
            "description font=\\normalfont, separator sign=\\hspace{\\customheadsep},",
            "segmentation style={black,solid,opacity=0.2,line width=1pt},",
            "colback=black!5!white, colbacktitle=black!5!white}{alg}"
        ],
        "description": "Framed boxes with tcolorbox"  
    },
    "steps": {
        "prefix": "tex-steps",
        "body": [
            "\\usepackage{enumitem}",
            "\\newlist{steps}{enumerate}{3}",
            "\\def\\stepindent{15pt}",
            "\\setlist[steps,1]{label=\\arabic*,widest=10,leftmargin=*,labelsep=\\stepindent,itemsep=0pt,topsep=5pt}",
            "\\setlist[steps,2]{label=\\arabic*,leftmargin=\\stepindent,labelindent=-\\stepindent,",
            "    labelsep=\\dimexpr\\stepindent*2\\relax,labelwidth=!,topsep=0pt,",
            "    before=\\setcounter{stepsii}{\\value{stepsi}},after=\\setcounter{stepsi}{\\value{stepsii}}}",
            "\\setlist[steps,3]{label=\\arabic*,leftmargin=\\stepindent,labelindent=-\\dimexpr\\stepindent*2\\relax,",
            "    labelsep=\\dimexpr\\stepindent*3\\relax,labelwidth=!,topsep=0pt,",
            "    before=\\setcounter{stepsiii}{\\value{stepsii}},after=\\setcounter{stepsii}{\\value{stepsiii}}}",
            "\\newlist{inout}{description}{1}",
            "\\setlist[inout,1]{labelindent=\\stepindent,widest=Output:,leftmargin=*,",
            "labelsep=.5em,topsep=5pt,font={\\normalfont\\itshape},itemsep=0pt}",
            "\\newcommand\\parsepitem{\\item[]\\nointerlineskip}"
        ],
        "description": "Steps for algorithms"
      }
}
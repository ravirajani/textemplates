{
    "maths": {
        "prefix": "tex-maths",
        "body": [
            "\\usepackage{amsmath,amsthm}",
            "\\renewcommand\\dotsc{\\mathinner{\\ldotp\\kern-0.12em\\ldotp\\kern-0.12em\\ldotp}} % Tighter ellipsis between commas",
            "\\DeclareMathOperator{\\vol}{vol}",
            "\\usepackage{newtxtext,newtxmath} % Times font",
            "\\usepackage{mleftright} % \\mleft \\mright has no extra space around it",
            "\\usepackage{bm}",
            "\\newcommand\\dotp{\\mathbin{\\bm{\\cdot}}}",
            "",
            "\\newlength{\\customheadsep}",
            "\\setlength{\\customheadsep}{1.5ex plus 1.5ex minus .3ex}",
            "",
            "\\usepackage{xpatch}",
            "\\makeatletter",
            "\\xpatchcmd{\\proof}{\\@addpunct{.}}{}{}{}",
            "\\makeatother",
            "\\xpatchcmd{\\proof}{\\trivlist}{\\setlength{\\labelsep}{\\customheadsep}\\trivlist}{}{}",
            "\\xpatchcmd{\\proof}{\\itshape}{\\itshape\\bfseries}{}{}",
            "\\renewcommand{\\qedsymbol}{$\\blacksquare$}",
            "",
            "\\newtheoremstyle{plainthm}% name",
            "{}%      Space above, empty = `usual value'",
            "{}%      Space below",
            "{\\itshape}% Body font",
            "{}%         Indent amount (empty = no indent, \\parindent = para indent)",
            "{\\bfseries}% Thm head font",
            "{}%        Punctuation after thm head",
            "{\\customheadsep}%     Space after thm head: \" \" = normal interword space;",
            "      %       \\newline = linebreak",
            "{\\thmname{#1}\\thmnumber{ #2}\\thmnote{ \\normalfont{#3}}}% Thm head spec",
            "",
            "\\theoremstyle{plainthm}",
            "\\newtheorem{theorem}{Theorem}[section]",
            "",
            "\\newtheoremstyle{boldthm}% name",
            "{}%      Space above, empty = `usual value'",
            "{}%      Space below",
            "{\\itshape}% Body font",
            "{}%         Indent amount (empty = no indent, \\parindent = para indent)",
            "{\\bfseries}% Thm head font",
            "{}%        Punctuation after thm head",
            "{\\customheadsep}%     Space after thm head: \" \" = normal interword space;",
            "      %       \\newline = linebreak",
            "{\\thmname{#1}\\thmnumber{ #2}\\thmnote{ (#3)}}% Thm head spec",
            "",
            "\\theoremstyle{boldthm}",
            "\\newtheorem{btheorem}[theorem]{Theorem}",
            "",
            "\\newtheoremstyle{customex}% name",
            "{}%      Space above, empty = `usual value'",
            "{}%      Space below",
            "{}% Body font",
            "{}%         Indent amount (empty = no indent, \\parindent = para indent)",
            "{\\bfseries}% Thm head font",
            "{}%        Punctuation after thm head",
            "{\\customheadsep}%     Space after thm head: \" \" = normal interword space;",
            "      %       \\newline = linebreak",
            "{\\thmname{#1}\\thmnumber{ #2}\\thmnote{ (#3)}}% Thm head spec",
            "",
            "\\theoremstyle{customex}",
            "\\newtheorem{examplex}[theorem]{Example}",
            "\\newenvironment{example}%",
            "    {\\pushQED{\\qed}\\renewcommand{\\qedsymbol}{$\\blacktriangle$}\\examplex}%",
            "    {\\popQED\\endexamplex}",
            "",
            "\\numberwithin{equation}{section}",
            "\\numberwithin{figure}{section}",
            "",
            "\\usepackage{caption,floatrow}",
            "\\DeclareCaptionLabelSeparator{capspace}{\\hspace{\\customheadsep}}",
            "\\DeclareFloatStyle{figure}{capposition=bottom}",
            "\\floatsetup[figure]{style=figure}",
            "\\captionsetup[figure]{font=small,labelfont=bf,labelsep=capspace}",
            "",
            "\\usepackage{nicematrix}",
            "\\NiceMatrixOptions{renew-dots,renew-matrix,nullify-dots}",
            "",
            "\\usepackage{xfrac} % small fraction \\sfrac defined here",
            "",
            "\\usepackage{enumitem}",
            "\\newlist{parts}{enumerate}{2}",
            "\\setlist[parts,1]{label=\\normalfont{(\\alph*)},labelsep=.5em}",
            "\\setlist*[parts,2]{label=\\normalfont{(\\roman*)},labelsep=.5em}"
        ],
        "description": "Maths"
    },
    "titlesec": {
        "prefix": "tex-titlesec",
        "body": [
            "\\usepackage[small,center]{titlesec}",
            "\\titleformat*{\\subsection}{\\filcenter\\itshape\\bfseries}",
            "\\titleformat{\\subsubsection}[runin]",
            "{\\normalfont\\bfseries}",
            "{\\thesubsection}{.33em}{}[.]",
            "\\titlespacing{\\subsubsection}{0pt}{1.5ex plus .1ex minus .2ex}{.5em}"
        ],
        "description": "Custom titles"
    },
    "complexity": {
        "prefix": "tex-complexity",
        "body": [
            "\\usepackage[classfont=sanserif,",
            "            langfont=caps,",
            "            funcfont=roman]{complexity}",
            "\\newlang{\\SVP}{SVP}"
        ],
        "description": "Complexity classes"
    },
    "algorithm": {
        "prefix": "tex-algorithm",
        "body": [
            "% Requires packages: floatrow,caption,enumitem",
            "\\usepackage{algorithm,varwidth}",
            "\\usepackage{algorithmicx}",
            "\\algnewcommand{\\linegap}{\\vspace{.5\\baselineskip}}",
            "\\newcounter{ALG@block}",
            "\\def\\algblock{0}",
            "\\algnewcommand{\\Statel}[1]{\\item[\\alglinenumber{#1}]}",
            "\\algnewcommand{\\Block}{%",
            "    \\def\\algblock{1}",
            "}",
            "\\makeatletter",
            "\\algnewcommand{\\multiline}[1]{%",
            "    \\begin{varwidth}[t]{\\dimexpr\\linewidth-\\ALG@thistlm}#1\\end{varwidth}",
            "    \\linegap",
            "}",
            "\\algnewcommand{\\BlockedAlgorithm}{%",
            "    \\setcounter{ALG@block}{0}",
            "    \\def\\ALG@step{%",
            "        \\ifthenelse{\\equal{\\algblock}{1}}{%",
            "            \\addtocounter{ALG@block}{1}%",
            "            \\setcounter{ALG@line}{\\value{ALG@block}}%",
            "            \\alglinenumber{\\arabic{ALG@block}}%",
            "            \\global\\def\\algblock{0}%",
            "        }{}%",
            "    }%",
            "}",
            "\\patchcmd{\\ALG@doentity}{\\item[]\\nointerlineskip}{}{}{}% fixes spacing bug in algorithmicx",
            "\\makeatother",
            "\\algdef{SN}[WHILE]{While}{EndWhile}[1]{\\textbf{while}\\ #1:}%",
            "\\algdef{SN}[FOR]{For}{EndFor}[1]{\\textbf{for}\\ #1:}%",
            "\\algdef{SN}[IF]{If}{EndIf}[1]{\\textbf{if}\\ #1:}%",
            "\\algdef{SE}[DO]{Do}{EndDo}{\\textbf{do}:}[1]{\\textbf{while}\\ #1}%",
            "\\DeclareFloatStyle{algorithm}{",
            "capposition=top,",
            "midcode=rule,",
            "postcode=lowrule,",
            "heightadjust=all}",
            "\\floatsetup[algorithm]{style=algorithm}",
            "\\captionsetup[algorithm]{singlelinecheck=false,labelfont=bf,labelsep=period}",
            "\\newlist{steps}{enumerate}{2}",
            "\\setlist[steps,1]{label=\\textbf{Step \\arabic*:},wide,labelindent=0pt,labelsep=1ex,ref=\\arabic*}",
            "\\setlist*[steps,2]{%",
            "    label=\\normalfont{\\textbf{\\alph{stepsii}.}},%",
            "    labelsep=1.5ex,%",
            "    ref=\\arabic{stepsi}\\alph{stepsii}}"
        ],   
        "description": "Custom algorithm pseudocode"
    },
    "summary": {
        "prefix": "tex-summary",
        "body": [
            "\\newenvironment{summary}",
            "{\\leftskip\\parindent\\relax\\rightskip\\parindent\\relax\\noindent\\ignorespaces}",
            "{\\par}"
        ],
        "description": "Summary environment definition"
    }
}